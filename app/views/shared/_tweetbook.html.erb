<% content_for(:header_tags) do %>
  <%= stylesheet_link_tag(:tweetbook, :plugin => 'redmine_tweetbook') %>
<% end %>

<% unless $tweetbook_settings['twitter']['key'].blank? || $tweetbook_settings['twitter']['secret'].blank? %>
  <%= link_to image_tag("twitterBtn.png", :plugin => "redmine_tweetbook"), "/auth/twitter", :class => "signin-button" %>
<% end %>
<% unless $tweetbook_settings['facebook']['key'].blank? || $tweetbook_settings['facebook']['secret'].blank? %>
  <%= link_to image_tag("facebookBtn.png", :plugin => "redmine_tweetbook"), "/auth/facebook", :class => "signin-button" %>
<% end %>
<% unless $tweetbook_settings['github']['key'].blank? || $tweetbook_settings['github']['secret'].blank? %>
  <%= link_to image_tag("githubBtn.jpg", :plugin => "redmine_tweetbook"), "/auth/github", :class => "signin-button" %>
<% end %>
